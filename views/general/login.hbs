<body>
  
<br>
<div class="log">
  <h1 class="t1">Welcome Back!</h1>
  <h2 class="t1">Log into your account</h2>
  <br>
  <form id="login-form" action="/login" method="POST" >
    <fieldset>
      <legend>Log In</legend>
      
   <br>
    <label class="info" for="email">Email Address</label>
    <input type="email" id="email" name="email" placeholder="Enter Your Email Address" value="{{values.email}}" spellcheck="false">
    {{{Check "email" validationMessages}}}
<br>
    <label class="info" for="password">Password</label>
    <input type="password" id="password" name="password"  placeholder="Enter Your Password" value="{{values.password}}" spellcheck="false">
    {{{Check "password" validationMessages}}}
<br>
<br>

     <div class="form-reg">
                    <input type="submit" value="Log In" class="btn-reg">
                </div>
</fieldset>
</form>
</div>
<br>

  
</body>